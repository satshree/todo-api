generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Todo {
  id          Int      @id @default(autoincrement())
  title       String
  color       String
  level       Int      @default(1)
  completed   Boolean  @default(false)
  timestamp   DateTime @default(now())
}